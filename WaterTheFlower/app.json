{
  "expo": {
    "name": "WaterTheFlowers",
    "slug": "WaterTheFlowers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/flower.png",
    "splash": {
      "image": "./assets/giris.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "useNextNotificationsApi": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/flower.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "app.watertheflowers.App"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
